{
    "manifest_version": 3,
    "name": "My TSUP App",
    "version": "0.1.0",
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [{
      "matches": ["*://*.quora.com/*"],
      "js": ["content.js"]
    }],
    "action": {
      "default_popup": "popup.html"
    },
    "options_ui": {
      "page": "options.html"
    }
  }
  